{
  "name": "secure-router",
  "version": "3.1.2",
  "description": "Helping you write more secure express apps.",
  "keywords": [
    "express",
    "security"
  ],
  "main": "build/index.js",
  "author": "Good Eggs Inc.",
  "contributors": [
    "Max Edmands <max@goodeggs.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/goodeggs/secure-router.git"
  },
  "scripts": {
    "prepublish": "yarn build",
    "build": "yarn run build:clean && yarn run build:transpile && yarn run build:types",
    "build:transpile": "babel --extensions=.ts,.js,.jsx,.tsx src --out-dir build --copy-files",
    "build:clean": "rm -rf build",
    "build:types": "tsc --project tsconfig.declarations.json",
    "test:mocha": "yarn run test:mocha:glob 'src/**/*.test.ts'",
    "test:mocha:glob": "NODE_ENV=test mocha --opts mocha.opts --extension ts,js,jsx,tsx",
    "test:watch": "yarn test:mocha -- --watch",
    "test": "yarn lint && yarn run typecheck && yarn test:mocha",
    "lint": "yarn run lint:src ",
    "lint:src": "yarn run lint:src:glob '**/*.{js,jsx,ts,tsx}'",
    "lint:src:glob": "getk run lint-es --ignore-path .gitignore",
    "lint:fix": "getk run fix-es --ignore-path .gitignore",
    "typecheck": "tsc"
  },
  "dependencies": {
    "async": "^3.2.4",
    "bluebird": "^3.7.2",
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "path-to-regexp": "^1.5.3",
    "router": "^1.3.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@goodeggs/toolkit": "^7.0.1",
    "@goodeggs/tsconfig": "^2.0.2",
    "@types/async": "^3.2.20",
    "@types/bluebird": "^3.5.38",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "^10.0.1",
    "@types/request": "^2.48.8",
    "express": "^4.18.2",
    "in-publish": "^2.0.0",
    "mocha": "^10.2.0",
    "request": "^2.73.0",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
