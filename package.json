{
  "name": "secure-router",
  "version": "1.0.0",
  "description": "Helping you write more secure express apps.",
  "keywords": [
    "express",
    "security"
  ],
  "main": "index.js",
  "author": "Good Eggs Inc.",
  "contributors": [
    "Max Edmands <max@goodeggs.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/goodeggs/goodeggs-event-processing-helper.git"
  },
  "scripts": {
    "build": "rm -rf build/ && babel src -d build && cp package.json can_publish build/",
    "prepublish": "./can_publish",
    "postpublish": "npm cache clean",
    "test:mocha": "NODE_ENV=test mocha --compilers=js:babel-register --require=babel-polyfill test.js",
    "test:watch": "npm run test:mocha -- --watch",
    "test": "npm run lint && npm run test:mocha --",
    "lint": "eslint '**/*.js'  --ignore-path .gitignore"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "lodash": "^4.13.1",
    "path-to-regexp": "^1.5.3",
    "router": "^1.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.0.1",
    "eslint-plugin-goodeggs": "^3.2.0",
    "eslint-plugin-lodash": "^1.9.4",
    "eslint-plugin-mocha": "^4.0.0",
    "express": "^4.14.0",
    "in-publish": "^2.0.0",
    "mocha": "^2.5.3",
    "request": "^2.73.0"
  },
  "eslintConfig": {
    "plugins": [
      "goodeggs"
    ],
    "extends": [
      "plugin:goodeggs/goodeggs"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "prefer-reflect": 0
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "pubishConfig": {
    "registry": "https://npmjs.com"
  }
}
